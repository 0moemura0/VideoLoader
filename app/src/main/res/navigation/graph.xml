<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph.xml"
    app:startDestination="@id/videoListFragment">
    
    <fragment
        android:id="@+id/videoListFragment"
        android:name="com.omoemurao.videoloader.VideoListFragment"
        android:label="fragment_video_list"
        tools:layout="@layout/fragment_video_list" >
        <action
            android:id="@+id/action_videoListFragment_to_addVideoFragment"
            app:destination="@id/addVideoFragment" />
    </fragment>
    <fragment
        android:id="@+id/addVideoFragment"
        android:name="com.omoemurao.videoloader.AddVideoFragment"
        android:label="fragment_add_video"
        tools:layout="@layout/fragment_add_video" />
</navigation>